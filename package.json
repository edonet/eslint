{
  "name": "@ainc/eslint-config-recommended",
  "version": "0.1.1",
  "description": "ESLint config recommended!",
  "types": "dist/index.d.ts",
  "main": "dist/index.js",
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "clean": "rm -rf ./dist",
